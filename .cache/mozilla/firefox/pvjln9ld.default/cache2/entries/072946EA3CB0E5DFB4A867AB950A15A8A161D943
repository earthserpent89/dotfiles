<span class="s">#exec_always --no-startup-id compton --config ~/.config/compton/compton.conf -b</span>
<span class="s">#exec --no-startup-id nm-applet</span>
<span class="s">exec --no-startup-id feh --bg-scale /home/username/Pictures/line.jpg</span>
