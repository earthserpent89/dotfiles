<span class="k">set</span><span class="n"> </span><span class="o">$</span><span class="n">Locker i3lock &amp;&amp; sleep </span><span class="mi">1</span>
<span class="k">set</span><span class="n"> </span><span class="o">$</span><span class="n">mode_system System</span><span class="o">:</span><span class="n">   L </span><span class="o">:</span><span class="n">  | S </span><span class="o">:</span><span class="n">  | P </span><span class="o">:</span><span class="n">  | R </span><span class="o">:</span><span class="n">  | E </span><span class="o">:</span><span class="n"> </span>

