<span class="n">    bindsym l exec </span><span class="nf">--no-startup-id</span><span class="n"> i3lock-fancy, mode </span><span class="s">&quot;default&quot;</span>
<span class="n">    bindsym s exec </span><span class="nf">--no-startup-id</span><span class="n"> </span><span class="o">$</span><span class="n">Lock systemctl suspend, mode </span><span class="s">&quot;default</span>
<span class="s">    bindsym p exec --no-startup-id systemctl poweroff -i, mode &quot;</span><span class="n">default</span><span class="s">&quot;</span>
<span class="s">    bindsym r exec --no-startup-id systemctl reboot, mode &quot;</span><span class="n">default</span><span class="s">&quot;</span>
<span class="s">    bindsym e exec --no-startup-id i3-msg exit, mode &quot;</span><span class="n">default</span><span class="s">&quot;</span>

<span class="s">#   back to normal: Enter or Escape</span>
