<span class="k">alias</span><span class="n"> </span><span class="k">monitor5</span><span class="o">=</span><span class="s">&#39;cmatrix -bC magenta&#39;</span>
<span class="k">alias</span><span class="n"> </span><span class="k">hora</span><span class="o">=</span><span class="s">&#39;tty-clock -cstDC red&#39;</span>

<span class="k">export</span><span class="n"> </span><span class="k">LC_ALL</span><span class="o">=</span><span class="n">es_VE.UTF-8</span>

<span class="c1">#	Colors:</span>

<span class="c1">#  BLACK=	&#39;\e[0;30m&#39;</span>
<span class="c1">#  RED=		&#39;\e[0;31m&#39;</span>
<span class="c1">#  GREEN=	&#39;\e[0;32m&#39;</span>
<span class="c1">#  YELLOW=	&#39;\e[0;33m&#39;</span>
<span class="c1">#  BLUE=	&#39;\e[0;34m&#39;</span>
