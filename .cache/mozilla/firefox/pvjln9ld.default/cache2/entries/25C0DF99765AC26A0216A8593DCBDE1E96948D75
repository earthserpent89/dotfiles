<span class="c1"># align</span>
<span class="c1"># border</span>
<span class="c1"># color</span>
<span class="c1"># command</span>
<span class="c1"># full_text</span>
<span class="c1"># instance</span>
<span class="c1"># interval</span>
<span class="c1"># label</span>
<span class="c1"># min_width</span>
<span class="c1"># name</span>
<span class="c1"># separator</span>
<span class="c1"># separator_block_width</span>
<span class="c1"># short_text</span>
<span class="c1"># signal</span>
<span class="c1"># urgent</span>

<span class="c1"># Global properties</span>
<span class="c1">#</span>
<span class="c1"># The top properties below are applied to every block, but can be overridden.</span>
<span class="c1"># Each block command defaults to the script name to avoid boilerplate.</span>
<span class="k">command</span><span class="o">=</span><span class="n">/usr/libexec/i3blocks/</span><span class="o">$</span><span class="n">BLOCK_NAME</span>

<span class="k">full_text</span><span class="o">=</span><span class="n"></span>
<span class="k">align</span><span class="o">=</span><span class="n">center</span>
<span class="k">separator</span><span class="o">=</span><span class="kp">false</span>
<span class="k">separator_block_width</span><span class="o">=</span><span class="mi">6</span>
<span class="c1">#color=#e7dfd9</span>
<span class="k">markup</span><span class="o">=</span><span class="n">pango</span>

<span class="k">border_left</span><span class="o">=</span><span class="mi">0</span>
<span class="k">border_right</span><span class="o">=</span><span class="mi">0</span>
<span class="k">border_top</span><span class="o">=</span><span class="mi">0</span>
<span class="k">border_bottom</span><span class="o">=</span><span class="mi">3</span>
<span class="k">border</span><span class="o">=</span><span class="s">#0d1116</span>

<span class="k">[focused</span><span class="n"> window]</span>
<span class="k">command</span><span class="o">=</span><span class="n">xtitle </span><span class="nf">-s</span><span class="n"> </span><span class="nf">-r</span>
<span class="k">interval</span><span class="o">=</span><span class="n">persist</span>
<span class="k">border</span><span class="o">=</span><span class="s">#64727e</span>

<span class="nx">[weather]</span>
<span class="k">instance</span><span class="o">=</span><span class="mi">3647651</span>
<span class="k">command</span><span class="o">=</span><span class="n">/usr/lib/i3blocks/weather</span>
<span class="k">interval</span><span class="o">=</span><span class="mi">60</span>
<span class="k">markup</span><span class="o">=</span><span class="n">pango</span>
<span class="k">border</span><span class="o">=</span><span class="s">#0073a0</span>

<span class="nx">[disk-home]</span>
<span class="k">label</span><span class="o">=</span><span class="n"></span>
<span class="k">command</span><span class="o">=</span><span class="n">/usr/lib/i3blocks/disk </span><span class="o">$</span><span class="n">HOME</span>
<span class="k">instance</span><span class="o">=</span><span class="n">/home/virus69</span>
<span class="k">interval</span><span class="o">=</span><span class="mi">30</span>
