<span class="c1">#Shortcuts</span>
<span class="k">bindsym</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">F1 exec /usr/bin/firefox</span>
<span class="k">bindsym</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">F2 exec /usr/bin/chromium</span>
