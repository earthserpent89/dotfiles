<span class="c1"># Global properties</span>
<span class="k">align</span><span class="o">=</span><span class="n">center</span>
<span class="k">color</span><span class="o">=</span><span class="s">#93a1a1</span>
<span class="k">separator_block_width</span><span class="o">=</span><span class="mi">0</span>
<span class="k">separator</span><span class="o">=</span><span class="kp">false</span>
