<span class="k">command</span><span class="o">=</span><span class="n">echo </span><span class="s">&quot;&lt;span background=\&quot;#021215\&quot; foreground=\&quot;#2aa198\&quot;&gt;$(date &#39;+%A, %d %b %Y&#39;) &lt;/span&gt;&quot;</span>
<span class="k">interval</span><span class="o">=</span><span class="mi">3600</span>

