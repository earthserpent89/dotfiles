<span class="k">no-dock-shadow</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="kp">false</span><span class="o">;</span><span class="n">		</span><span class="c1"># Avoid drawing shadows on dock/panel windows.</span>
<span class="k">no-dnd-shadow</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="kp">true</span><span class="o">;</span><span class="n">		</span><span class="c1"># Don&#39;t draw shadows on DND windows.</span>
<span class="k">clear-shadow</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="kp">true</span><span class="o">;</span><span class="n">		</span><span class="c1"># Zero the part of the shadow&#39;s mask behind the window (experimental).</span>
<span class="k">shadow-radius</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="mi">12</span><span class="o">;</span><span class="n">		</span><span class="c1"># The blur radius for shadows. (default 12)</span>
<span class="k">shadow-offset-x</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="mi">-15</span><span class="o">;</span><span class="n">		</span><span class="c1"># The left offset for shadows. (default -15)</span>
