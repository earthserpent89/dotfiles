<span class="o">;</span><span class="n"> </span><span class="k">visible</span><span class="n"> </span><span class="o">=</span><span class="n"> Active workspace on unfocused monitor</span>
<span class="k">label-visible</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">%</span><span class="n">index</span><span class="o">%</span>
<span class="k">label-visible-background</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">${</span><span class="n">self.label-focused-background</span><span class="o">}</span>
<span class="k">label-visible-underline</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">${</span><span class="n">self.label-focused-underline</span><span class="o">}</span>
