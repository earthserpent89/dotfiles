<span class="o">;</span><span class="n"> </span><span class="k">urgent</span><span class="n"> </span><span class="o">=</span><span class="n"> Workspace with urgency hint set</span>
<span class="k">label-urgent</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">%</span><span class="n">index</span><span class="o">%</span>
<span class="k">label-urgent-background</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">${</span><span class="n">module/bspwm.label-urgent-background</span><span class="o">}</span>
