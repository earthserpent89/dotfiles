<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;mpv|Mplayer|baka-mplayer&quot;]</span><span class="n">   </span><span class="o">$</span><span class="n">ws4</span>
<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;Thunar&quot;]</span><span class="n">   </span><span class="o">$</span><span class="n">ws4</span>

<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;YACReaderLibrary&quot;]</span><span class="n"> </span><span class="o">$</span><span class="n">ws5</span>
<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;YACReader&quot;]</span><span class="n"> </span><span class="o">$</span><span class="n">ws5</span>
<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;^zathura$&quot;]</span><span class="n"> </span><span class="o">$</span><span class="n">ws5</span>
