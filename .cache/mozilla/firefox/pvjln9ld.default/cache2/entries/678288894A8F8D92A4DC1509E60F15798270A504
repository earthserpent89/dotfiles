<span class="c1"># open applications on specific workspaces</span>
<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;^URxvt$&quot;]</span><span class="n"> </span><span class="o">$</span><span class="n">ws1</span>
<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;Subl3&quot;]</span><span class="n">   </span><span class="o">$</span><span class="n">ws2</span>

<span class="k">assign</span><span class="n"> </span><span class="nx">[class=&quot;^Wicd&quot;]</span><span class="n">   </span><span class="o">$</span><span class="n">ws3</span>
