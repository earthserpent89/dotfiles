<span class="nx">[colors]</span>
<span class="k">background</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="c1">#222</span>
<span class="k">background-alt</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="c1">#444</span>
