<span class="k">format-warn</span><span class="n"> </span><span class="o">=</span><span class="n"> &lt;ramp&gt; &lt;label-warn&gt;</span>
<span class="k">format-warn-underline</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">${</span><span class="n">self.format-underline</span><span class="o">}</span>

<span class="k">label</span><span class="n"> </span><span class="o">=</span><span class="n">   </span><span class="o">%</span><span class="n">temperature</span><span class="o">%</span>
<span class="k">labeil-foreground</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="s">#0088CC</span>
<span class="k">label-warn</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">%</span><span class="n">temperature</span><span class="o">%</span>
<span class="k">label-warn-foreground</span><span class="n"> </span><span class="o">=</span><span class="n"> </span><span class="o">${</span><span class="n">colors.secondary</span><span class="o">}</span>

<span class="k">ramp-0</span><span class="n"> </span><span class="o">=</span><span class="n"> </span>
<span class="k">ramp-1</span><span class="n"> </span><span class="o">=</span><span class="n"> </span>
<span class="k">ramp-2</span><span class="n"> </span><span class="o">=</span><span class="n"> </span>
