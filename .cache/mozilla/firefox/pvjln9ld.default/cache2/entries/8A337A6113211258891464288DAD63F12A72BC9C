<span class="o">###########</span><span class="n"></span>
<span class="o">#</span><span class="n"> i3</span><span class="o">-</span><span class="n">Gaps </span><span class="o">#</span><span class="n"></span>
<span class="o">###########</span><span class="n"></span>

<span class="o">#</span><span class="n"> Borders </span><span class="o">#</span><span class="n"></span>
<span class="n">for</span><span class="o">_</span><span class="n">window </span><span class="err">[</span><span class="n">class</span><span class="o">=</span><span class="s">&quot;^.*&quot;</span><span class="err">]</span><span class="n"> border pixel 4</span>

<span class="o">#</span><span class="n"> Set inner</span><span class="o">/</span><span class="n">outer gaps </span><span class="o">#</span><span class="n"></span>
<span class="n">gaps inner 15</span>
<span class="n">gaps outer 10</span>

<span class="o">#</span><span class="n"> Smart gaps </span><span class="err">(</span><span class="n">gaps used if only more than one container on the workspace</span><span class="err">)</span><span class="n"> </span><span class="o">#</span><span class="n"></span>
<span class="o">#</span><span class="n"> smart</span><span class="o">_</span><span class="n">gaps on</span>

<span class="o">#</span><span class="n"> Smart borders </span><span class="err">(</span><span class="n">draw borders around container only if it is not the only container on this workspace</span><span class="err">)</span><span class="n"> </span><span class="o">#</span><span class="n"> </span>
<span class="n">smart</span><span class="o">_</span><span class="n">borders on</span>

<span class="n">workspace </span><span class="err">$</span><span class="n">ws1 gaps inner 0</span>
<span class="n">workspace </span><span class="err">$</span><span class="n">ws1 gaps outer 0</span>

