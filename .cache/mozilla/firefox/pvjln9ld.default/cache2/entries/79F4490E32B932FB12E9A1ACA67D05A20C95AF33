<span class="k">client.unfocused</span><span class="n">            </span><span class="s">#1A1A1A</span><span class="n"> </span><span class="s">#1A1A1A</span><span class="n"> </span><span class="s">#747474</span><span class="n"> </span><span class="s">#1A1A1A</span>
<span class="k">client.urgent</span><span class="n">               </span><span class="s">#1A1A1A</span><span class="n"> </span><span class="s">#EA4C59</span><span class="n"> </span><span class="s">#a5a5a5</span><span class="n"> </span><span class="s">#1A1A1A</span>
<span class="k">client.background</span><span class="n">           </span><span class="s">#121212</span>


<span class="c1">#############################</span>
<span class="c1"># hide/unhide i3status bar</span>
