<span class="k">bindsym</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">s floating toggle</span>

<span class="k">mode</span><span class="n"> </span><span class="s">&quot;resize&quot;</span><span class="n"> </span><span class="o">{</span>
<span class="n">    bindsym Left resize shrink width </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>
<span class="n">    bindsym Down resize grow height </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>
