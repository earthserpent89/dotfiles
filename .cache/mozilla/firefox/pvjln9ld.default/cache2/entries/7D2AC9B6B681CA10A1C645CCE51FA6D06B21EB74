<span class="k">CPREV</span><span class="o">=</span><span class="s">&quot;\ue055&quot;</span>
<span class="k">CNEXT</span><span class="o">=</span><span class="s">&quot;\ue05b&quot;</span>
<span class="k">CPLAY</span><span class="o">=</span><span class="s">&quot;\ue058&quot;</span>
