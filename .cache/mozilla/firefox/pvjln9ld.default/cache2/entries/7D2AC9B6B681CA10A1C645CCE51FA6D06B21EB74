<span class="k">YELLOW2</span><span class="o">=</span><span class="s">&quot;$(getXresColor color11)&quot;</span>
<span class="k">BLUE</span><span class="o">=</span><span class="s">&quot;$(getXresColor color4)&quot;</span>
